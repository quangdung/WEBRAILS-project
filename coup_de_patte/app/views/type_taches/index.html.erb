<h1>Listing Type Taches</h1>

<table class="table table-striped table-hover table-bordered table-responsive">
  <thead>
    <tr>
      <th>Nom</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @type_taches.each do |type_tach| %>
      <tr>
        <td><%= type_tach.nom %></td>
        <td>
          <%= link_to 'Show', type_tach %>
          <%= link_to 'Edit', edit_type_tach_path(type_tach) %>
          <%= link_to 'Destroy', type_tach, method: :delete, data: { confirm: 'Are you sure?' }, :class => 'btn btn-sm btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type tache', new_type_tach_path %>
